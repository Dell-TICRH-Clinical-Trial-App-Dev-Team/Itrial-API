{
  "name": "itrial-api",
  "version": "1.0.0",
  "description": "api for web client and mobile app for ITrial",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "ENV_FILE=./config/.env.prod node ./dist/app.js",
    "local": "ENV_FILE=./config/.env.defaults ts-node-dev src/app.ts",
    "dev": "ENV_FILE=./config/.env.dev ts-node src/app.ts",
    "test": "ENV_FILE=./config/.env.test jest",
    "lint": "prettier --write",
    "devWindows": "ts-node src/app.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dell-TICRH-Clinical-Trial-App-Dev-Team/Itrial-API.git"
  },
  "keywords": [
    "express",
    "mongoose",
    "typescript"
  ],
  "author": "Shaham Noorani, Joseph Jin, Nathaniel Li",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dell-TICRH-Clinical-Trial-App-Dev-Team/Itrial-API/issues"
  },
  "homepage": "https://github.com/Dell-TICRH-Clinical-Trial-App-Dev-Team/Itrial-API#readme",
  "devDependencies": {
    "@types/dotenv-parse-variables": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.2",
    "@types/faker": "^5.5.7",
    "@types/jest": "^26.0.24",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.4.1",
    "@types/supertest": "^2.0.11",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "prettier": "2.3.2",
    "supertest": "^6.1.4",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "dotenv-extended": "^2.9.0",
    "dotenv-parse-variables": "^2.0.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-jwt-authz": "^2.4.1",
    "jwks-rsa": "^2.0.4",
    "mongoose": "^5.13.3",
    "morgan": "^1.10.0"
  }
